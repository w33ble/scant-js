{
  "name": "@scant/router",
  "version": "0.0.4",
  "description": "Dead simple router",
  "main": "dist/router.js",
  "module": "dist/router.es.js",
  "browser": "dist/router.min.js",
  "author": "Joe Fleming (https://github.com/w33ble)",
  "repository": "https://github.com/w33ble/scant-js/tree/master/packages/router",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "version": "npm run build",
    "test": "tape 'test/index.js'",
    "build": "scant-build --ext url-pattern:Path"
  },
  "scant-build": {
    "moduleName": "router",
    "external": [
      "url-pattern"
    ],
    "global": {
      "url-pattern": "Path"
    },
    "format": [
      "es",
      "umd",
      "umd-min"
    ],
    "filename": "router[suffix].js",
    "banner": true
  },
  "dependencies": {
    "url-pattern": "^1.0.3"
  },
  "devDependencies": {
    "@scant/build-helper": "^0.0.1",
    "tape": "^4.8.0"
  }
}
