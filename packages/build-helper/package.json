{
  "name": "@scant/build-helper",
  "private": true,
  "version": "0.0.1",
  "description": "Build helper for @scant packages",
  "bin": {
    "scant-build": "dist/cli.js"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && bili"
  },
  "author": "Joe Fleming (https://github.com/w33ble)",
  "repository": "https://github.com/w33ble/scant-js/tree/master/packages/build-helper",
  "license": "MIT",
  "bili": {
    "format": [
      "cjs"
    ],
    "input": "src/{index,cli}.js",
    "filename": "[name].js",
    "inline": false
  },
  "dependencies": {
    "@std/esm": "^0.18.0",
    "bili": "^2.2.7",
    "rimraf": "^2.6.2",
    "use-config": "^2.0.4"
  }
}
