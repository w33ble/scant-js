{
  "name": "@scant/emitter",
  "version": "0.2.2",
  "main": "dist/emitter.js",
  "module": "dist/emitter.es.js",
  "browser": "dist/emitter.min.js",
  "author": "Joe Fleming <github.com/w33ble>",
  "repository": "https://github.com/w33ble/scant-js/tree/master/packages/emitter",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": ["dist"],
  "scripts": {
    "version": "npm run build",
    "test": "ava",
    "build": "scant-build"
  },
  "ava": {
    "require": ["esm"]
  },
  "esm": {
    "cjs": true
  },
  "scant-build": {
    "moduleName": "emitter",
    "format": ["es", "umd", "umd-min"],
    "filename": "emitter[suffix].js",
    "banner": true
  },
  "devDependencies": {
    "@scant/build-helper": "^0.0.1",
    "ava": "^0.24.0",
    "esm": "^3.0.28"
  },
  "dependencies": {}
}
